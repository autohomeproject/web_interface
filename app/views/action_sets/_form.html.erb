<div class="grid_4">
        <div class="block-border">
                <div class="block-header">
                        <p align="right" ><%= link_to 'Show', @action_sets %> | <%= link_to 'Back', rooms_path %>&nbsp;&nbsp;&nbsp;</p>
                </div>
<%= form_for(@action_set, :html => { :class => "block-content form"}) do |f| %>
  <% if @action_set.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@action_set.errors.count, "error") %> prohibited this action_set from being saved:</h2>

      <ul>
      <% @action_set.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </div>
  <div class="field">
    <%= f.label :user_id %><br />
    <%= collection_select :action_set, :user_id, User.all, :id, :name, { :prompt => 'Please Select User' } %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
<div class="clear"></div>
        </div>
</div>

